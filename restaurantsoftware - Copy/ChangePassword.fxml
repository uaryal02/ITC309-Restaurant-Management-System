<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="180.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="restaurantsoftware.ChangePasswordController">
   <children>
      <Button layoutX="81.0" layoutY="130.0" mnemonicParsing="false" onAction="#savePassword" text="Save Password" />
      <PasswordField fx:id="oldPasswordText" layoutX="26.0" layoutY="25.0" promptText="Old Password" />
      <PasswordField fx:id="newPasswordText" layoutX="26.0" layoutY="60.0" promptText="New Password" />
      <PasswordField fx:id="confirmPasswordText" layoutX="26.0" layoutY="95.0" promptText="Confirm Password" />
      <Button layoutX="26.0" layoutY="130.0" mnemonicParsing="false" onAction="#back" text="Back" />
   </children>
</AnchorPane>